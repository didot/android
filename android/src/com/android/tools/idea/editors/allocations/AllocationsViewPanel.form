<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.android.tools.idea.editors.allocations.AllocationsViewPanel">
  <grid id="27dc6" binding="myContainer" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="99024" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="8d3ab" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="7f297" class="com.intellij.ui.components.JBCheckBox" binding="myGroupingCheckBox">
                <constraints/>
                <properties>
                  <margin top="2" left="2" bottom="2" right="2"/>
                  <text value="Group by allocated class"/>
                </properties>
              </component>
            </children>
          </grid>
          <grid id="13cc2" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="746f" class="com.intellij.ui.components.JBLabel">
                <constraints/>
                <properties>
                  <text value="Filter:"/>
                </properties>
              </component>
              <component id="45cf2" class="com.intellij.ui.components.JBTextField" binding="myFilterField">
                <constraints/>
                <properties>
                  <preferredSize width="128" height="23"/>
                </properties>
              </component>
              <component id="2a2cf" class="com.intellij.ui.components.JBCheckBox" binding="myIncludeTraceCheckBox" default-binding="true">
                <constraints/>
                <properties>
                  <text value="Include trace"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <splitpane id="ab63a">
        <constraints border-constraint="Center"/>
        <properties>
          <dividerLocation value="158"/>
          <orientation value="0"/>
        </properties>
        <border type="none"/>
        <children>
          <scrollpane id="5b9c5" class="com.intellij.ui.components.JBScrollPane" binding="myAllocationsPane">
            <constraints>
              <splitpane position="left"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="6963d" class="com.intellij.ui.table.JBTable" binding="myAllocationsTable">
                <constraints/>
                <properties>
                  <autoCreateRowSorter value="false"/>
                </properties>
              </component>
            </children>
          </scrollpane>
          <scrollpane id="61891" class="com.intellij.ui.components.JBScrollPane">
            <constraints>
              <splitpane position="right"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="1c69b" class="javax.swing.JEditorPane" binding="myStackTraceEditorPane">
                <constraints/>
                <properties>
                  <contentType value="text/html"/>
                  <editable value="false"/>
                </properties>
              </component>
            </children>
          </scrollpane>
        </children>
      </splitpane>
    </children>
  </grid>
</form>
