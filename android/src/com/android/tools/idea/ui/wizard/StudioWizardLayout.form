<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.android.tools.idea.ui.wizard.StudioWizardLayout">
  <grid id="27dc6" binding="myRootPanel" layout-manager="BorderLayout" hgap="0" vgap="15">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="8aea9" binding="myHeaderPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="empty">
          <size top="20" left="10" bottom="20" right="10"/>
        </border>
        <children>
          <component id="780db" class="com.android.tools.idea.ui.ImageComponent" binding="myIcon">
            <constraints/>
            <properties/>
          </component>
          <grid id="29858" binding="myTitlePanel" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="0" left="0" bottom="0" right="0"/>
            <constraints/>
            <properties>
              <opaque value="false"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="9a0c9" class="com.intellij.ui.components.JBLabel" binding="myProductLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <foreground color="-1"/>
                  <text value="Android Studio"/>
                </properties>
              </component>
              <component id="21ec7" class="com.intellij.ui.components.JBLabel" binding="myTitleLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="10" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="24"/>
                  <foreground color="-1"/>
                  <text value="(Wizard Title)"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <grid id="d5619" binding="myCenterPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="empty">
          <size top="0" left="12" bottom="12" right="12"/>
        </border>
        <children/>
      </grid>
    </children>
  </grid>
</form>
