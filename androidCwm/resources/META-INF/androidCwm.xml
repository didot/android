<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <backendProjectPaneProvider implementation="org.intellij.android.cwm.BackendAndroidPaneProvider"/>
    <toolWindowExtractorMode id="Logcat" mode="mirror"/>

    <!-- We want to convert DeviceAndSearchPanel, but treat it in the code as its public supertype JPanel,
         because target type DeviceAndSearchPanel is package-private -->
    <rdserver.converter implementationClass="org.intellij.android.cwm.DeviceAndSearchPanelConverter"
                        key="com.android.tools.idea.logcat.DeviceAndSearchPanel"/>
  </extensions>
</idea-plugin>
