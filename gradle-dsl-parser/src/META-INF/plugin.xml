<idea-plugin>
  <id>com.android.tools.gradle-dsl-parser</id>
  <name>Gradle DSL Parser</name>
  <version>1.0</version>
  <vendor>Google</vendor>
  <description>Provides a parser implementation for gradle DSL buildfiles</description>

  <idea-version since-build="145.597"/>

  <extensions defaultExtensionNs="com.android.tools">
    <gradleModelProvider implementation="com.android.tools.idea.gradle.dsl.api.GradleModelSource"/>
  </extensions>
</idea-plugin>
